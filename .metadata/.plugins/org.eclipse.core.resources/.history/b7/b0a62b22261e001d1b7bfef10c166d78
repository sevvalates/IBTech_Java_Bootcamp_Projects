package com.godoro.geometry;

public class Rectangle {

	int width;
	int height;
	
	Rectangle(int width,int height){
		this.width=width;
		this.height=height;
	}
	Rectangle(){
		this.width=1;
		this.height=1;
	}

	int getArea() {
		return width*height;
	}
	int getPerimeter() {
		return 2*(width+height);
	}

}
