package com.godoro.sales;

public class Customer {

	private int customerId;
	private String customerName;
	private int totalDebit;
	
	public Customer(int salesCustomer, int customerId, String customerName, int totalDebit) {
		this.salesCustomer = salesCustomer;
		this.customerId = customerId;
		this.customerName = customerName;
		this.totalDebit = totalDebit;
	}

	public Customer() {
	}

	public int getSalesCustomer() {
		return salesCustomer;
	}

	public void setSalesCustomer(int salesCustomer) {
		this.salesCustomer = salesCustomer;
	}

	public int getCustomerId() {
		return customerId;
	}

	public void setCustomerId(int customerId) {
		this.customerId = customerId;
	}

	public String getCustomerName() {
		return customerName;
	}

	public void setCustomerName(String customerName) {
		this.customerName = customerName;
	}

	public int getTotalDebit() {
		return totalDebit;
	}

	public void setTotalDebit(int totalDebit) {
		this.totalDebit = totalDebit;
	}
	
	
}
